marcrdf:ind2
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain marcrdf:DataField ;
      rdfs:label "Second Indicator"@en-US ;
      rdfs:range xsd:string ;
      vitro:descriptionAnnot
              "The first and second indicators are optional single-character values associated with a standard MARC field which modify the meaning of the field in a way defined differently for each field. For example, the second indicator in a bibliographic MARC record's 245 (title) field indicates the number of characters at the beginning of the title that should be disregarded w/r/t alphabetization, offering generalized support for avoiding alphabetizing by initial articles in any language (as long as the maximum single-digit 9-character offset is sufficient)."^^xsd:string ;
      vitro:displayRankAnnot
              "3"^^xsd:int ;
      vitro:exampleAnnot "3"^^xsd:string ;
      vitro:hiddenFromDisplayBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:inPropertyGroupAnnot
              <http://vitro.mannlib.cornell.edu/ns/default#n7791> ;
      vitro:prohibitedFromUpdateBelowRoleLevelAnnot
